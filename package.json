    "react-dropdown": "^1.2.1",
    "react-dropzone": "^5.0.1",
    "react-hot-loader": "4.3.6",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.1.1",
    "react-select": "^2.0.0",
    "redux": "^4.0.0",
    "redux-action-buffer": "^1.1.0",
    "redux-logger": "^3.0.6",
